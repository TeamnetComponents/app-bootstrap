<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql,mssql"/>
    <property name="now" value="sysdate" dbms="oracle"/>

    <changeSet id="160720151" author="bootstrap" dbms="!mssql">
        <createTable tableName="T_UPLOAD_FILE_LOG">
            <column name="id_upload_file_log" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="file_location" type="varchar(255)"/>
            <column name="token" type="varchar(100)"/>
            <column name="upload_date" type="timestamp"/>
            <column name="status" type="bigint"/>
        </createTable>

    </changeSet>

    <changeSet id="160720151#mssql" author="bootstrap" dbms="mssql">
        <createTable tableName="T_UPLOAD_FILE_LOG">
            <column name="id_upload_file_log" type="bigint" autoIncrement="false">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="file_location" type="varchar(255)"/>
            <column name="token" type="varchar(100)"/>
            <column name="upload_date" type="timestamp"/>
            <column name="status" type="bigint"/>
        </createTable>

    </changeSet>
</databaseChangeLog>
